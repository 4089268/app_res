<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.salvador.restaurant_v01" >

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/icono_principal"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name=".Principal"
            android:label="@string/title_activity_principal" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".menu"
            android:label="@string/title_activity_menu" >
        </activity>
        <activity
            android:name=".subMenu"
            android:label="@string/title_activity_sub_menu" >
        </activity>
        <activity
            android:name=".descripcionPromocion"
            android:label="@string/title_activity_descripcion_promocion" >
        </activity>
        <activity
            android:name=".descripComida"
            android:label="@string/title_activity_descrip_comida" >
        </activity>
        <activity
            android:name=".recervacion"
            android:label="@string/title_activity_recervacion" >
        </activity>

    </application>

</manifest>
